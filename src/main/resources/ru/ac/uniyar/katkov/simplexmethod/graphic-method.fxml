           dividerPositions="0.19583333333333333" prefHeight="160.0" prefWidth="200.0">
    <items>
        <VBox prefHeight="766.0" prefWidth="308.0" styleClass="containers">
            <children>
                <HBox styleClass="containers">
                    <children>
                        <Button mnemonicParsing="false" onAction="#decreaseGraphScale" text="-"/>
