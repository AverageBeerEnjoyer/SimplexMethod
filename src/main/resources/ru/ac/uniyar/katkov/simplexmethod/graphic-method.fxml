                    </children>
                </HBox>
                <VBox fx:id="unequals" prefHeight="200.0" prefWidth="100.0" styleClass="containers"/>
            </children>
        </VBox>
        <Pane prefHeight="746.0" prefWidth="1321.0" styleClass="beer-vibe">
            <children>
                <Canvas fx:id="canvas" height="746.0" width="1154.0"/>
            </children>
        </Pane>
    </items>
